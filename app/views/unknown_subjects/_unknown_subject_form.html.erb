<%# unknown_subject_form %>
<%= form_for unknown_subject do |f| %>
  <fieldset>
    <legend>New Unknown Subject</legend>
    <%= f.label :name %>
    <%= f.text_field :name, :placeholder => 'Unknown subject name' %>
    <%= f.label :description %>
    <%= f.text_area :description, :placeholder => 'Unknown subject description' %>
    <br/>
    <%= f.submit %>
<% end -%>
